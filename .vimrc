inoremap jj <Esc>
set autoindent expandtab tabstop=4 shiftwidth=4
set relativenumber
colorscheme darkblue
highlight Normal ctermbg=none
let mapleader =  
nmap <leader>e :w<CR>:E<CR>
nmap <leader>bn :w<CR>:bn<CR>
nmap <leader>bp :w<CR>:bp<CR>
nmap <leader>j 23j
nmap <leader>k 23k
nmap <leader>n :nohl<CR>
nmap <leader>o o<Esc>
nmap <leader>O O<Esc>
